{
    "name": "passcore",
    "version": "1.0.0",
    "description": "PassCore is a self-service password change utility for Active Directory.",
    "main": "index.html",
    "scripts": {
        "lint": "tsc --noEmit && eslint ./**/*.{js,ts,tsx}  --quiet",
        "lint:fix": "eslint ./**/*.{js,ts,tsx}  --quiet --fix",
        "build": "parcel build index.html --dist-dir ../wwwroot",
        "build:tsc": "tsc",
        "prettier": "prettier --check ./**/*.ts*",
        "prettier:fix": "prettier --write ./**/*.ts*",
        "start": "parcel index.html --dist-dir ../wwwroot --open --port 4040"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/unosquare/passcore.git"
    },
    "author": "Unosquare",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/unosquare/passcore/issues"
    },
    "homepage": "https://github.com/unosquare/passcore",
    "dependencies": {
        "@mui/icons-material": "^6.4.5",
        "@mui/material": "^6.4.5",
        "@mui/styles": "^6.4.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-form-validator-core": "^2.0.2",
        "uno-js": "^0.7.32",
        "zxcvbn": "^4.4.2"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@types/react": "^17.0.83",
        "@types/react-dom": "^17.0.26",
        "@types/zxcvbn": "^4.4.0",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "8.5.0",
        "eslint-config-standard-react": "^13.0.0",
        "eslint-config-standard-with-typescript": "^23.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "4.2.1",
        "eslint-plugin-promise": "^6.0.1",
        "eslint-plugin-react": "^7.31.8",
        "globals": "^16.0.0",
        "minimist": "^1.2.8",
        "node-forge": "^0.10.0",
        "parcel": "^2.13.3",
        "parcel-plugin-clean-easy-unsafe": "^1.0.2",
        "prettier": "^2.7.1",
        "ts-standard": "^10.0.0",
        "typescript": "~4.2.3"
    },
    "parcelCleanPaths": [
        "../wwwroot"
    ],
    "parcelCleanUnsafePaths": true
}
